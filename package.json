{
  "name": "ts-rpa",
  "version": "0.1.1",
  "description": "npm install -g ts-node",
  "main": "dist/index.js",
  "files": [
    "dist/**/*",
    "src/**/*.ts"
  ],
  "bin": {
    "ts-rpa": "dist/bin.js"
  },
  "scripts": {
    "sample": "ts-node src/sample.ts",
    "bin": "ts-node src/bin.ts",
    "build": "rimraf ./dist && require-self && tsc",
    "doc": "rimraf ./docs && typedoc",
    "prepare": "npm run build",
    "lint": "eslint '+(src|tests)/**' --ext .ts",
    "fix": "eslint '+(src|tests)/**' --ext .ts --fix",
    "coverage": "codecov",
    "test": "jest -w 1"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "./dist/",
      "./node_modules/",
      "./coverage/"
    ],
    "transform": {
      "^.+\\.(t|j)sx?$": "ts-jest"
    },
    "testRegex": "/tests/.*\\.spec\\.(ts|js)$",
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "coverageDirectory": "./coverage/",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.ts?(x)"
    ],
    "globals": {
      "ts-jest": {
        "tsConfig": "tsconfig.json"
      }
    }
  },
  "pre-commit": [
    "lint"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@slack/web-api": "^5.0.1",
    "@types/node": "^12.0.5",
    "@types/node-fetch": "^2.3.7",
    "@types/node-forge": "^0.8.3",
    "@types/nodemailer": "^6.2.0",
    "@types/selenium-webdriver": "^4.0.0",
    "commander": "^2.20.0",
    "csv": "^5.1.1",
    "csv-parse": "^4.4.3",
    "csv-writer": "^1.3.0",
    "file-type": "^12.0.1",
    "form-data": "^2.4.0",
    "gaxios": "^2.0.1",
    "googleapis": "^40.0.0",
    "googleapis-common": "^2.0.0",
    "iconv-lite": "^0.4.24",
    "is-utf8": "^0.2.1",
    "log4js": "^4.3.1",
    "mime-stream": "^1.0.1",
    "node-fetch": "^2.6.0",
    "node-forge": "^0.8.4",
    "nodemailer": "^6.2.1",
    "pkginfo": "^0.4.1",
    "require-self": "^0.2.1",
    "rimraf": "^2.6.3",
    "selenium-webdriver": "^4.0.0-alpha.1",
    "typescript": "^3.5.1",
    "unzipper": "^0.10.0"
  },
  "devDependencies": {
    "@types/jest": "^24.0.13",
    "@typescript-eslint/eslint-plugin": "^1.9.0",
    "@typescript-eslint/parser": "^1.9.0",
    "codecov": "^3.5.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-import-resolver-typescript": "^1.1.1",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jest": "^22.6.4",
    "eslint-plugin-prettier": "^3.1.0",
    "jest": "^24.8.0",
    "pre-commit": "^1.2.2",
    "prettier": "^1.17.1",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.2.0",
    "typedoc": "^0.14.2",
    "typedoc-plugin-localization": "^1.4.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ca-rpa/ts-rpa.git"
  },
  "bugs": {
    "url": "https://github.com/ca-rpa/ts-rpa/issues"
  },
  "homepage": "https://github.com/ca-rpa/ts-rpa#readme"
}
